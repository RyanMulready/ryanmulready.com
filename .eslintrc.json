{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "@nuxtjs/eslint-config-typescript",
        "plugin:nuxt/recommended",
        "airbnb-base",
        "plugin:prettier/recommended"
    ],

    "rules": {
        "prettier/prettier": ["error"],
        "import/no-unresolved": "off",
        "@typescript-eslint/no-unused-vars": "off"
    },
    "globals": {
        "NodeJS": true,
        "$fetch": true
    }
}
